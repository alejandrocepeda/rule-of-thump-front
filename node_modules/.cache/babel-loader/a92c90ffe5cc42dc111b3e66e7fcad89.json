AAA,oBAEA,KAAA,CAAA,QAFA;AAAA;AAAA;AAAA;;AAGA,gBAAA,KAAA,CAAA,MAAA,CAAA,MAAA,CAAA,gBAAA,EAAA,IAAA;;AAHA;;AAAA;AAOA,gBAAA,MAPA,GAOA;AACA,kBAAA,OAAA,EAAA,KAAA,CAAA,IAAA,CAAA,EADA;AAEA,kBAAA,OAAA,EAAA,KAAA,CAAA,QAAA,CAAA,IAAA,CAAA,EAFA;AAGA,kBAAA,UAAA,EAAA,KAAA,CAAA,YAAA,CAAA,IAHA;AAIA,kBAAA,aAAA,EAAA,KAAA,CAAA,YAAA,CAAA;AAJA,iBAPA;AAAA;AAAA,uBAcA,KAAA,CAAA,IAAA,CAAA,aAAA,EAAA,MAAA,CAdA;;AAAA;AAcA,gBAAA,QAdA;;AAgBA,gBAAA,KAAA,CAAA,KAAA,CAAA;AACA,kBAAA,KAAA,EAAA,KAAA,CAAA,EAAA,CAAA,0CAAA,CADA;AAEA,kBAAA,IAAA,EAAA,KAAA,CAAA,EAAA,CAAA,wCAAA,CAFA;AAGA,kBAAA,IAAA,EAAA,UAHA;AAIA,kBAAA,WAAA,EAAA,SAJA;AAKA,kBAAA,gBAAA,EAAA,IALA;AAMA,kBAAA,kBAAA,EAAA,mCANA;AAOA,kBAAA,iBAAA,EAAA,kCAPA;AAQA,kBAAA,kBAAA,EAAA,SARA;AASA,kBAAA,iBAAA,EAAA,SATA;AAUA,kBAAA,iBAAA,EAAA,KAAA,CAAA,EAAA,CAAA,iBAAA;AAVA,iBAAA;;AAeA,gBAAA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,SAAA;;AA/BA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAgCA;AAzCA;AAtBA,CAAA","sourcesContent":["<template>\n    <div style=\"padding-bottom: 35px;\" class=\"text-white col-md-6\">\n        <div class=\"position-relative\" >\n        \n\n            <img height=\"620\" :src=\"post.image_url\"  style=\"width:100%; object-fit: cover;\">\n                \n            <div class=\"overlay-post\"></div>\n\n            <a style=\"left:0; top:57%;\" class=\"cursor-pointer position-absolute mr-3 rounded-0 btn-sm btn btn-success\" href=\"#\" >                \n                <i class=\"mdi mdi-18px mdi-thumb-up\"></i>\n            </a>\n            \n            <div class=\"centered pl-5\">\n\n                <div class=\"row\">\n                    <div class=\"col\">\n                        \n                        <h1 style=\"line-height: 25px;font-weight: 700;\">{{ post.title }} <br>\n                            <small class=\"text-white\" style=\"font-size: 35%;\"> <span class=\"font-weight-bold\">{{ post.date | fromNow  }} </span> in {{ post.category.name }}</small>\n                        </h1>\n\n                        <p class=\"pt-2\" style=\"color:#E8E8E7\">{{ post.description }}</p>\n                        <p>\n\n                            <a :class=\"{'border-vote':voteThisPost.like}\" @click.prevent=\"voteLike()\" class=\"mr-3 rounded-0 btn-sm btn btn-success\" >\n                                <i class=\"mdi mdi-18px mdi-thumb-up\"></i>\n                            </a>\n\n                            <a :class=\"{'border-vote':voteThisPost.dislike}\" @click.prevent=\"voteDisLike()\" class=\"mr-3 rounded-0 btn-sm btn btn-warning\">\n                                <i class=\"mdi mdi-18px mdi-thumb-down\"></i>\n                            </a>\n\n                            <a v-show=\"voteThisPost.dislike || voteThisPost.like\"  @click.prevent=\"voteNow()\" class=\"rounded-0 btn border btn-outline\" href=\"#\" role=\"button\">{{ $t('Vote now')}}</a>\n                        </p>\n                        \n                    </div>\n                </div>   \n            </div>          \n\n           \n                \n            <PostVoteResults :post=\"post\"/> \n\n            \n            \n        </div>\n    </div>\n</template>\n\n<script>\nimport PostVoteResults from '@/components/PostVoteResults'\nimport moment from 'moment'\nimport { mapState } from 'vuex'\n\nexport default {\n    components:{\n        PostVoteResults\n    },\n    name: 'Post',\n    data(){\n        return {\n            voteThisPost:{\n                like: false,\n                dislike: false,\n            }\n        }\n    },\n    computed:{\n        ...mapState(['isLogged','authUser'])\n    },    \n    props:{\n        post:{\n            type: Object,\n            required: true\n        }\n    },\n    methods:{\n        voteDisLike(){\n            this.voteThisPost.dislike = !this.voteThisPost.dislike\n            this.voteThisPost.like = false\n        },\n        voteLike(){\n            this.voteThisPost.like = !this.voteThisPost.like\n            this.voteThisPost.dislike = false\n        },\n        async voteNow(){\n\n            if(!this.isLogged){\n                this.$store.commit('SET_OPEN_MODAL', true)\n                return \n            }\n\n            const toPost = {\n                post_id: this.post.id,\n                user_id: this.authUser.user.id,\n                like_count: this.voteThisPost.like,\n                dislike_count: this.voteThisPost.dislike\n            }\n\n            const response = await axios.post('users-votes',toPost)\n\n            this.$swal({\n                title: this.$t('Are you sure to retranscode this video ?'),\n                text: this.$t(\"¡You will not be able to reverse this!\"),\n                type: 'question',\n                customClass: 'bg-body',\n                showCancelButton: true,\n                confirmButtonClass: 'btn btn-success font-weight-light',\n                cancelButtonClass: 'btn btn-danger font-weight-light',\n                confirmButtonColor: '#53D190',\n                cancelButtonColor: '#F65F6E',\n                confirmButtonText: this.$t('¡Yes, continue!')\n            })\n\n\n            \n            this.$store.dispatch('getPots')\n        }\n    }\n}\n</script>\n\n<style >\n    .border-vote{\n        border: 2px #fff solid !important;\n    }\n\n</style>"],"sourceRoot":"src/components"}]}